<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.cloud9.android.happycode">

    <!-- To access Google+ APIs: -->
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_cloud9_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartPageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".QuestionActivity">
        </activity>
        <activity android:name=".ResultPageActivity">
        </activity>
        <activity android:name=".AboutPageActivity">
        </activity>
        <activity android:name=".CodesActivity">
        </activity>
        <activity android:name=".TestHistoryActivity">
        </activity>
        <activity android:name=".LogInActivity">
        </activity>
        <activity android:name=".EqualScores">
        </activity>
        <activity android:name=".EqualScoreActivity">
        </activity>
    </application>

</manifest>