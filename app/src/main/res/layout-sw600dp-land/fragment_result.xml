<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/black"
    >

    <TextView
        android:id="@+id/text_view_result_header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="20dp"
        android:layout_marginTop="16dp"
        android:textSize="40sp"
        android:textStyle="bold"
        android:textColor="@android:color/white"
        android:text="@string/result_title"
        />

    <LinearLayout
        android:id="@+id/linearlayout_result_images"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/text_view_result_header"
        android:orientation="horizontal"
        >

        <ImageView
            android:id="@+id/imageview_result_one"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginLeft="16dp"
            android:src="@drawable/ic_android_happy_code_24dp"
            android:contentDescription="image"
            />

        <ImageView
            android:id="@+id/imageview_result_two"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_weight="1"
            android:src="@drawable/ic_android_happy_code_24dp"
            android:contentDescription="image"
            />

        <ImageView
            android:id="@+id/imageview_result_three"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginRight="16dp"
            android:src="@drawable/ic_android_happy_code_24dp"
            android:contentDescription="image"
            />

    </LinearLayout>


    <ImageView
        android:id="@+id/imageview_result_line"
        android:layout_width="6dp"
        android:layout_height="15dp"
        android:layout_below="@id/linearlayout_result_images"
        android:layout_marginLeft="59dp"
        android:layout_marginRight="59dp"
        android:background="@drawable/line_result_page"
        android:contentDescription="image"
        />

    <TextView
        android:id="@+id/textview_result_strenght_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/linearlayout_result_images"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/box_result_page"
        android:padding="16dp"
        android:textColor="@android:color/white"
        android:textAlignment="center"
        android:text="Title"
        android:textSize="30sp"
        tools:textSize="36sp"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/textview_result_strentgh_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/textview_result_strenght_title"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:background="@drawable/box_result_page"
        android:padding="16dp"
        android:textColor="@android:color/white"
        android:textAlignment="center"
        android:textSize="19sp"
        tools:textSize="19sp"
        android:text="Lorem ipsum dolor sit amet, consectetur adipiscing elit. Etiam mollis sit amet nisi nec posuere."
        />

    <LinearLayout
        android:id="@+id/result_button_linear_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="18dp"
        android:gravity="center">

        <Button
            android:text="@string/button_share"
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            style="@style/Buttons"
            android:id="@+id/button_share_result"
            android:layout_marginRight="18dp"
            />

        <Button
            android:id="@+id/button_result_to_menu"
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            style="@style/Buttons"
            android:text="@string/button_result_menu"
            android:textAlignment="center"
            />

    </LinearLayout>

